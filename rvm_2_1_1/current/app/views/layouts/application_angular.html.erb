 <div id="local-server" data-uri="<%= request.host %>:<%= ENV['SOCKET_PORT'] %>/websocket?user_id=<%= current_user.id %>"></div>
 <input type="hidden" value="<%= current_user.id %>" id="socket-id">
 <input type="hidden" value="<%= ENV['V2_LISTEN_URL'] %>" id="v2-listen" >
 <input type="hidden" value="<%= ENV['PIPELINE_URL'] %>" id="v2_pipeline">
 <input type="hidden" value="<%= ENV['PIPELINE_WS_URL'] %>" id="v2_pipeline_ws">
 <input type="hidden" value="<%= ENV['RECCOS_URL'] %>" id="v2_reccos">
 <input type="hidden" value="<%= ENV['PIWIK_URL'] %>" id="piwik_url">
 <input type="hidden" value="<%= ENV['PIWIK_SITE_ID'] %>" id="piwik_site_id">
 <input type="hidden" value="<%= ENV['PIWIK_TOKEN'] %>" id="piwik_token">

 <%= render :partial => "shared/js_files_link" %>
  </body>
</html>
